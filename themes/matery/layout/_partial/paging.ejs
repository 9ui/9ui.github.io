<div class="container paging">
    <% if (page.prev) { %>
    <a href="<%- url_for(page.prev_link) %>" class="center btn-large waves-effect waves-light bg-color page-btn">
        上一页
    </a>
    <% } %>
    <% if (page.next) { %>
    <a href="<%- url_for(page.next_link) %>" class="center  btn-large waves-effect waves-light bg-color page-btn">
        下一页
    </a>
    <% } %>
</div>